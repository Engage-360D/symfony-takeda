{
  "name": "takeda",
  "version": "0.0.1",
  "scripts" : {
    "postinstall": "./node_modules/.bin/bower --allow-root install"
  },
  "dependencies": {
    "bower": "~1.2",
    "grunt-contrib-coffee": "~0.8",
    "grunt-regarde": "~0.1",
    "grunt-contrib-connect": "~0.6",
    "grunt-contrib-clean": "~0.5",
    "grunt-exec": "~0.4",
    "grunt-react": "~0.6",
    "coffee-script": "~1.6.3",
    "gulp": "~3.5",
    "gulp-exec": "~1.0",
    "gulp-util": "~2.2",
    "gulp-coffee": "~1.3",
    "gulp-react": "~0.1",
    "gulp-browserify": "~0.3",
    "gulp-jade": "~0.4",
    "gulp-stylus": "~0",
    "gulp-concat": "~2.1",
    "gulp-connect": "~0.1",
    "gulp-imagemin": "~0.1",
    "gulp-uglify": "~0.2",
    "gulp-if": "0.0.5",
    "gulp-rename": "~1.2",
    "optimist": "~0.6",
    "reactify": "~0.4",
    "coffeeify": "~0.5",
    "through2": "~0.4.0",
    "browserify-shim": "~2.0.10",
    "browserify": "~3.24",
    "components-jquery": "git://github.com/components/jquery.git#1.10.2",
    "react-tools": "~0.5",
    "moment": "~2.4",
    "font-awesome": "~4.0",
    "underscore": "~1.6",
    "Eventable": "git://github.com/madebymany/eventable.git",
    "sir-trevor": "git://github.com/madebymany/sir-trevor-js.git"
  },
  "shim": {
    "jquery": {
      "path": "./node_modules/components-jquery/jquery.js",
      "exports": "$"
    },
    "React": {
      "path": "./node_modules/react-tools/build/modules/React.js",
      "exports": null
    },
    "moment": {
      "path": "./node_modules/moment/moment.js",
      "exports": null
    },
    "momentLang": {
      "path": "./node_modules/moment/lang/ru.js",
      "exports": null
    },
    "JSOjs": {
      "path": "./node_modules/JSOjs/jso.js",
      "exports": null,
      "depends": {
        "jquery": "jQuery"
      }
    },
    "jStorage": {
      "path": "./node_modules/jStorage/jstorage.js",
      "exports": null,
      "depends": {
        "jquery": "jQuery"
      }
    },
    "underscore": {
      "path": "./node_modules/underscore/underscore.js",
      "exports": null
    },
    "Eventable": {
      "path": "./node_modules/Eventable/eventable.js",
      "exports": "Eventable"
    },
    "sirTrevor": {
      "path": "./node_modules/sir-trevor/sir-trevor.js",
      "exports": null,
      "depends": {
        "underscore": "_",
        "jquery": "jQuery",
        "Eventable": "Eventable"
      }
    }
  }
}
