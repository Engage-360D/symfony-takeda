{
  "$schema": "http://json-schema.org/draft-04/schema#",

  "type": "object",

  "properties": {
    "specialization": {
      "oneOf": [
        {"type": "string"},
        {"type": "null"}
      ]
    },

    "experience": {
      "oneOf": [
        {"type": "number"},
        {"type": "null"}
      ]
    },

    "address": {
      "oneOf": [
        {"type": "string"},
        {"type": "null"}
      ]
    },

    "phone": {
      "oneOf": [
        {"type": "string"},
        {"type": "null"}
      ]
    },

    "institution": {
      "oneOf": [
        {"type": "string"},
        {"type": "null"}
      ]
    },

    "graduation": {
      "oneOf": [
        {"type": "string", "format": "date-time"},
        {"type": "null"}
      ]
    },

    "confirmSubscription": {
      "type": "boolean"
    },

    "firstname": {
      "type": "string"
    },

    "lastname": {
      "type": "string"
    },

    "email": {
      "type": "string"
    }
  }
}
