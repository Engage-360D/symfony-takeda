{
  "$schema": "http://json-schema.org/draft-04/schema#",

  "type": "object",

  "properties": {
    "tokens": {
      "type": "object",

      "properties": {
        "email": {
          "type": "string",
          "format": "email"
        },

        "plainPassword": {
          "type": "string"
        }
      },

      "additionalProperties": false,

      "required": [
        "email",
        "plainPassword"
      ]
    }
  },

  "required": [
    "tokens"
  ]
}
