{% extends "Engage360dTakedaBundle::layout.html.twig" %}

{% block title %}Мои рекомендации{% endblock %}

{% block section %}
<div class="container">
  <div class="head">
    <div class="head__right">
      <div class="head__city">
        {% include "Engage360dTakedaBundle::geo_ip_city.html.twig" %}
      </div>
      <div class="head__out">
        <a href="/sign-out">Выход</a>
      </div>
    </div>
    <div class="head__left">
      <div class="h">{{app.user.fullName}}</div>
    </div>
  </div>
  {{ include('Engage360dTakedaBundle:Account:tabs.html.twig') }}
  <div id="TestResultList"></div>
</div>

{% endblock %}

{% block scripts %}
<script>
  takeda.render(takeda.TestResultList, 'TestResultList', {
    testResults: {{ testResults | serialize | raw }}
  });
</script>
{% endblock %}
